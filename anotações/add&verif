#{
    create table cursos(
    idcurso int(11) not null auto_increment,
    nome varchar(30) not null,
    descricao text,
    carga int(10) unsigned,
    totaaulas int(10) unique,
    ano year(4),
    primary key(idcurso)
    )default charset = utf8;

    describe cursos;

    insert into cursos
    (idcurso, nome, descricao, carga, totaulas, ano)
    values
