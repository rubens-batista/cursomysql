    Númerico{
        Inteiro{
            TinyInt
            SmallInt
            Int
            MediumInt
            BigInt
        }
        Real{
            Decimal
            Float
            Double
            Real
        }
        Lógico{
            Bit
            Boolean
        }
    }
    Data/Tempo{
        Date
        DateTime
        TimeStamp
        Time
        Year
    }
    Literal{
        Caractere{
            Char
            VarChar
        }
